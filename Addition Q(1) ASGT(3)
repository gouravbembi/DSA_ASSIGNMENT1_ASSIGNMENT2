#include<iostream>
#include<stack>
using namespace std;
int main(){
    stack<int>st;
    int arr[]={2,3,5,8,0,7};
    int n=6;
    int nse[n];
    nse[n-1]=-1;
    st.push(arr[n-1]);
    for(int i=n-2;i>=0;i--){
        while(st.size()>0 && st.top()>arr[i]){
            st.pop();
        }
        if(st.empty()){
            nse[i]=-1;
        }
        else{ nse[i]=st.top();}
        st.push(arr[i]);
    }
    for(int i=0;i<n;i++){
        cout<<arr[i]<<"->"<<nse[i]<<endl;
    }

}
