#include <iostream>
using namespace std;

class Node {
public:
    int data;
    Node* next;
    Node* prev;
    Node(int d) {
        data = d;
        next = NULL;
        prev = NULL;
    }
};

class DoublyLL {
public:
    Node* head;
    DoublyLL() {
        head = NULL;
    }

    void insertLast(int v) {
        Node* n = new Node(v);
        if (!head) head = n;
        else {
            Node* t = head;
            while (t->next) t = t->next;
            t->next = n;
            n->prev = t;
        }
    }

    int size() {
        int c = 0;
        Node* t = head;
        while (t) {
            c++;
            t = t->next;
        }
        return c;
    }
};

class CircularLL {
public:
    Node* head;
    CircularLL() {
        head = NULL;
    }

    void insertLast(int v) {
        Node* n = new Node(v);
        if (!head) {
            head = n;
            n->next = head;
        }
        else {
            Node* t = head;
            while (t->next != head) t = t->next;
            t->next = n;
            n->next = head;
        }
    }

    int size() {
        if (!head) return 0;
        int c = 0;
        Node* t = head;
        do {
            c++;
            t = t->next;
        } while (t != head);
        return c;
    }
};

int main() {
    int type;
    cout << "1. Doubly Linked List\n2. Circular Linked List\nChoose: ";
    cin >> type;

    DoublyLL d;
    CircularLL c;

    int n, v;
    cout << "Enter number of nodes: ";
    cin >> n;

    for (int i = 0; i < n; i++) {
        cin >> v;
        if (type == 1) d.insertLast(v);
        else c.insertLast(v);
    }

    if (type == 1) cout << d.size();
    else cout << c.size();

    return 0;
}
