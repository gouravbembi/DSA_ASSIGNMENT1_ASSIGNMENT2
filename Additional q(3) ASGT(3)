 #include <iostream>
#include <stack>
using namespace std;

int main() {
    int arr[] = {3, 1, 2, 5, 4, 6, 2, 3};
    int n = 8;
    int nge[n];
    stack<int> st;

    
    nge[n-1] = -1;
    st.push(arr[n-1]);

   
    for (int i = n - 2; i >= 0; i--) {
        while (!st.empty() && st.top() <= arr[i]) {
            st.pop();
        }
        if (st.empty()) {
            nge[i] = -1;
        } else {
            nge[i] = st.top();
        }
        st.push(arr[i]);
    }

    
    cout << "Next greater elements:\n";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " -> " << nge[i] << endl;
    }
}
