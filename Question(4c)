#include <iostream>

using namespace std;

int main() {
    char str[100];
    cout << "Enter a string: ";
    cin.getline(str, 100);

    int j = 0; 
    for (int i = 0; str[i] != '\0'; i++) {
        char ch = tolower(str[i]);
        if (ch != 'a' && ch != 'e' && ch != 'i' && ch != 'o' && ch != 'u') {
            str[j++] = str[i]; 
        }
    }
    str[j] = '\0'; 

    cout << "After removing vowels: " << str << endl;
    return 0;
}
