#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter size of matrix: ";
    cin >> n;


   
    cout << "Enter elements for lower triangle (row >= col):\n";
    for(int i=0; i<n; i++) {
        for(int j=0; j<=i; j++) {
            cin >> A[i][j];
            A[j][i] = A[i][j]; 
        }
    }

   
    cout << "\nSymmetric Matrix:\n";
    for(int i=0; i<n; i++) {
        for(int j=0; j<n; j++) {
            cout << A[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}
