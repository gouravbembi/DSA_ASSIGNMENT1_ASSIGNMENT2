#include <iostream>
using namespace std;

int findMissingLinear(int arr[], int n) {
    for(int i=0; i<n-1; i++) {
        if(arr[i] != i+1)   
            return i+1;
    }
    return n;  
}

int main() {
    int n;
    cout << "Enter n: ";
    cin >> n;

    int arr[100];
    cout << "Enter " << n-1 << " sorted elements (1..n with one missing):\n";
    for(int i=0; i<n-1; i++) cin >> arr[i];

    cout << "Missing number is: " << findMissingLinear(arr, n) << endl;
    return 0;
}
